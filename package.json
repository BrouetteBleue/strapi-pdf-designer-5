{
  "name": "strapi-pdf-designer",
  "version": "1.0.13",
  "description": "Strapi PDF Designer plugin",
  "keywords": [
    "strapi5",
    "strapi-plugin",
    "strapi-pdf-designer"
  ],
  "strapi": {
    "name": "strapi-pdf-designer",
    "description": "Description of Pdf Designer plugin",
    "kind": "plugin",
    "displayName": "Pdf Designer",
    "required": false
  },
  "scripts": {
    "build": "strapi-plugin build",
    "cypress": "npx cypress open"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.14",
    "@strapi/icons": "^2.0.0-rc.14",
    "dayjs": "^1.11.13",
    "decode-html": "^2.0.0",
    "destr": "^2.0.3",
    "html-to-text": "^9.0.5",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "puppeteer": "^24.13.0",
    "react-email-editor": "^1.7.11",
    "react-icons": "^5.4.0",
    "react-intl": "^6.8.9",
    "react-textarea-autosize": "^8.5.6",
    "striptags": "^3.2.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@strapi/sdk-plugin": "^5.2.8",
    "@strapi/strapi": "^5.6.0",
    "@strapi/typescript-utils": "^5.6.0",
    "@types/html-to-text": "^9.0.4",
    "@types/lodash": "^4.17.13",
    "@types/mustache": "^4.2.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-email-editor": "^1.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.6",
    "@strapi/strapi": "^5.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.1",
    "styled-components": "^6.1.13"
  },
  "author": {
    "name": "A Engie developer"
  },
  "maintainers": [
    {
      "name": "A Engie developer"
    }
  ],
  "engines": {
    "node": ">=16.x.x <=18.x.x",
    "npm": ">=9.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SomeDevelopper/strapi-pdf-designer.git"
  },
  "license": "MIT"
}